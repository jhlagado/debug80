{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Extension (HelloWorld)",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--folder-uri=${workspaceFolder}/examples/HelloWorld"
      ],
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "preLaunchTask": "yarn: build"
    },
    {
      "name": "Extension (Caverns)",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--folder-uri=${workspaceFolder}/examples/Caverns"
      ],
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "preLaunchTask": "yarn: build"
    }
  ]
}
